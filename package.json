{
  "name": "gulp-font2css",
  "version": "0.1.2",
  "description": "Encode font files as CSS using Gulp.",
  "repository": "goblindegook/gulp-font2css",
  "author": {
    "name": "Lu√≠s Rodrigues",
    "email": "hello@goblindegook.com",
    "url": "http://goblindegook.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "cd src; babel *.js -d ../lib",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm run build",
    "pretest": "eslint src/**/*.js",
    "test": "ava"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "keywords": [
    "base64",
    "css",
    "font-face",
    "gulpplugin",
    "localfont",
    "otf",
    "ttf",
    "web",
    "webfonts",
    "woff",
    "woff2"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "css": "^2.2.1",
    "eslint": "^1.10.3",
    "vinyl": "^2.2.0"
  },
  "dependencies": {
    "css-font-style-keywords": "^1.0.1",
    "css-font-weight-keywords": "^1.0.0",
    "css-font-weight-names": "^0.2.1",
    "mime": "^2.3.1",
    "plugin-error": "^1.0.1",
    "replace-ext": "^1.0.0",
    "through2": "^2.0.0"
  }
}
